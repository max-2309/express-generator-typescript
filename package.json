{
  "name": "express-generator-typescript",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node-dev ./bin/www",
    "build": "tsc",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest",
    "push:dev": "git push origin --all && git push origin --tags",
    "push:prod": "git push prod master && git push prod master --tags",
    "push:all": "npm run push:dev && npm run push:prod"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "ts-node-dev": "~1.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.10",
    "jest": "~26.6.3",
    "supertest": "~6.0.1",
    "ts-jest": "~26.4.4"
  }
}
